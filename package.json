{
  "name": "super-selector",
  "version": "2.0.0",
  "description": "An eval-free DOM Super Selector with plugin architecture for Chrome Extensions and web scraping - V2 Complete Rewrite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "npm run build:watch",
    "docs": "typedoc src --out docs"
  },
  "keywords": [
    "dom",
    "selector",
    "chrome-extension",
    "scraping",
    "typescript",
    "css-selector",
    "plugin-system",
    "extensible",
    "performance",
    "caching"
  ],
  "author": "darkwolf66",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkwolf66/super-selector.git"
  },
  "bugs": {
    "url": "https://github.com/darkwolf66/super-selector/issues"
  },
  "homepage": "https://github.com/darkwolf66/super-selector#readme",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  }
}
